<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.channabasaawa.qp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2">
            <intent-filter>
                <action android:name="com.example.channabasaawa.qp.Main2Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Main3Activity">
            <intent-filter>
                <action android:name="com.example.channabasaawa.qp.Main3Activity" />

                <category android:name="android.intent.category.DEFAULT3" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Quesionpaper"
            android:label="@string/title_activity_quesionpaper"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user1"
            android:label="@string/title_activity_user1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ViewUploadsActivity" />
        <activity android:name=".User2">
            <intent-filter>
                <action android:name="com.example.channabasaawa.qp.user2" />

                <category android:name="android.intent.category.DEFAULT2" />
            </intent-filter>
        </activity>
        <activity android:name=".Main4Activity" />
        <activity
            android:name=".Studdetails">


            <intent-filter>
                <action android:name="com.example.channabasaawa.qp.Studdetails" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>
    </application>

</manifest>